<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="vo.MemberVO" alias="member"/>
		<typeAlias type="vo.GoodVO" alias="good"/>
		<typeAlias type="vo.N_BoardVO" alias="n_board"/>
		<typeAlias type="vo.F_BoardVO" alias="f_board"/>
		<typeAlias type="vo.ReplyVO" alias="reply"/>
	</typeAliases> 
	
	<mappers>
		<mapper resource="config/mybatis/mapper/member.xml"/>
		<mapper resource="config/mybatis/mapper/academy.xml"/>
		<mapper resource="config/mybatis/mapper/address.xml"/>
		<mapper resource="config/mybatis/mapper/good.xml"/>
		<mapper resource="config/mybatis/mapper/bookmark.xml"/>
		<mapper resource="config/mybatis/mapper/n_board.xml" />
		<mapper resource="config/mybatis/mapper/f_board.xml" />
		<mapper resource="config/mybatis/mapper/reply.xml"/>
	</mappers>
</configuration>