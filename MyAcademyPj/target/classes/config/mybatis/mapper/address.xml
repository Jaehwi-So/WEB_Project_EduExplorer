<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="addr_mapper">

	<!-- 전체게시물 조회 -->
	<select id="city_list" resultType="vo.AddressVO">
		select addr_city from city_view
	</select>

	<select id="region_list" resultType="vo.AddressVO" parameterType="java.lang.String">
		select * from city_address where addr_city = #{city}
	</select>

	
</mapper>












